<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.parkingpeace.RatingsCommentsController" prefWidth="400" prefHeight="300" spacing="10">
    <TextArea fx:id="ratingsTextArea" promptText="Enter ratings" />
    <TextArea fx:id="commentsTextArea" promptText="Enter comments" />
    <Button fx:id="submitButton" text="Submit" onAction="#handleSubmitButton" />
</VBox>
