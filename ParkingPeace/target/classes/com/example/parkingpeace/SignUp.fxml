       <TextField fx:id="ageField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   
       <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="5" />
       <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
   
       <Label text="Are you a:" GridPane.columnIndex="0" GridPane.rowIndex="6" />
       <HBox spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="6">
          <children>
              <Button onAction="#handleLandlordButton" text="Landlord" />
              <Button onAction="#handleCustomerButton" text="Customer" />
          </children>
       </HBox>
   
       <VBox spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="7">
      <children>
              <Button onAction="#handleSignUpButton" text="Sign Up" />
              <Button onAction="#handleBackButton" text="Back" />
      </children>
