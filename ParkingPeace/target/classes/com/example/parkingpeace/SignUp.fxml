<GridPane alignment="center" hgap="10" prefHeight="400" prefWidth="300" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.example.parkingpeace.SignUpController">
   <children>
       <Label text="Sign Up" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
   
       <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
       <TextField fx:id="usernameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   
